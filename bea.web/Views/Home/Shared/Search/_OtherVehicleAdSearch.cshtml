@model Bea.Models.Search.Vehicles.AdOtherVehicleSearchModel

<h6><i class="  icon-pencil"></i>&nbsp; Filtrer les annonces</h6>
<div class="controls controls-row">
        @Html.DropDownListFor(model => model.FuelSelectedId, (IEnumerable<SelectListItem>)ViewBag.Fuels, "Energie", new { @class = "input-large" })
        @Html.DropDownListFor(Model => Model.MinYearSelectedId, (IEnumerable<SelectListItem>)ViewBag.Years, "Annee Min", new { @class = "input-medium" })
        @Html.DropDownListFor(Model => Model.MaxYearSelectedId, (IEnumerable<SelectListItem>)ViewBag.Years, "Annee Max", new { @class = "input-medium" })
        @Html.DropDownListFor(Model => Model.MinKmSelectedId, (IEnumerable<SelectListItem>)ViewBag.Kms, "Km Min", new { @class = "input-medium" })
        @Html.DropDownListFor(Model => Model.MaxKmSelectedId, (IEnumerable<SelectListItem>)ViewBag.Kms, "Km Max", new { @class = "input-medium" })
</div>
